package matchings

/**
 * The uniform distribution over permutations. 
 */
model NonUniformPermutation {
  random Permutation permutation 
  param RealVar variance ?: {1.0}
  
  laws {
    logf(permutation, variance) {
      - logFactorial(permutation.componentSize)*permutation.getConnections().get(0)
    }
  }
  
  generate(rand){
  	
  }
  
}
